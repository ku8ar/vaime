{
  "root": true,
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile" : "false",
    "ecmaVersion": "latest",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "sourceType": "module",
    "babelOptions": { "configFile": "./.babelrc" }
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  },
  "env": {
    "es6": true
  },
  "extends": "gatsby-standard",
  "rules": {
    "react/display-name": [0],
    "no-unused-vars": ["warn"]
  }
}